             RowBox[{"galaxies", "[", 
              RowBox[{"[", 
               RowBox[{"n", ",", "2"}], "]"}], "]"}]}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"n", ",", "1", ",", "1"}], "}"}]}], "]"}], "+", 
        RowBox[{"2.71", "/", "2"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"sv", ",", 
         RowBox[{"10", "^", 
          RowBox[{"-", "20"}]}], ",", 
         RowBox[{"10", "^", 
          RowBox[{"-", "30"}]}]}], "}"}]}], "]"}]}]}], "\[IndentingNewLine]", 
   
   RowBox[{"(*", 
