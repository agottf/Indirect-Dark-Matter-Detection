   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"dN", "/", "dlogx"}], ")"}], "/", "E"}], 
     RowBox[{"\[DifferentialD]", "E"}]}]}], TraditionalForm]],ExpressionUUID->
  "7b140b8b-9355-4f61-bc90-f35cd25c3553"]
}], "Text",
