Cell[BoxData[
 RowBox[{
  RowBox[{"fSpace", "[", 
   RowBox[{"min_", ",", "max_", ",", "steps_", ",", 
    RowBox[{"f_", ":", "Log"}]}], "]"}], ":=", 
  RowBox[{
   RowBox[{"InverseFunction", "[", 
    RowBox[{
     RowBox[{"ConditionalExpression", "[", 
      RowBox[{
       RowBox[{"f", "[", "#", "]"}], ",", 
       RowBox[{"min", "<", "#", "<", "max"}]}], "]"}], "&"}], "]"}], "/@", 
   RowBox[{"Range", "[", 
    RowBox[{
     RowBox[{"f", "@", "min"}], ",", 
     RowBox[{"f", "@", "max"}], ",", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"f", "@", "max"}], "-", 
        RowBox[{"f", "@", "min"}]}], ")"}], "/", 
      RowBox[{"(", "steps", ")"}]}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.8098144392998257`*^9, 3.809814439307939*^9}, {
